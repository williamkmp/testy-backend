{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".prettierrc": true,
        ".gitignore": true,
        "target": true,
        "README.md": true,
        "mvnw.cmd": true,
        ".mvn": true,
        "src/main/resources/META-INF": true,
        ".vscode": true
    },
    "hide-files.files": [
        ".prettierrc",
        ".gitignore",
        "target",
        "README.md",
        "mvnw.cmd",
        ".mvn",
        "src/main/resources/META-INF",
        ".vscode"
    ],
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.format.settings.url": ".vscode/eclipse-java-google-style.xml",
    "maven.view": "hierarchical",
    "java.compile.nullAnalysis.mode": "disabled",
    "java.debug.settings.onBuildFailureProceed": true,
    "java.dependency.packagePresentation": "hierarchical",
    "[java]": {
        "editor.defaultFormatter": "mwpb.java-prettier-formatter"
    },
    "java.inlayHints.parameterNames.enabled": "none",
    "prettier.printWidth": 60,
    "prettier.tabWidth": 4,
}